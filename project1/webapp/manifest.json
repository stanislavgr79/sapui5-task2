{
  "_version": "1.81.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.1",
      "toolsId": "de42c8e5-d17a-4bdf-a2fe-6abf13bda7e5"
    },
    "dataSources": {
      "ODataV2": {
        "uri": "/odata/v2/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/oDataV2/metadata.xml"
        }
      },
      "ODataV4": {
        "uri": "/odata/v4/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0",
          "localUri": "localService/oDataV4/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.144.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "v2": {
        "dataSource": "ODataV2",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {}
      },
      "v4": {
        "dataSource": "ODataV4",
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "project1.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteProduct",
          "pattern": "Product/{productId}",
          "target": [
            "TargetProduct"
          ]
        },
        {
          "name": "RouteTab",
          "pattern": "tab/{tabKey}",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "pages.main.Main"
        },
        "TargetProduct": {
          "id": "Product",
          "name": "pages.product.Product"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
